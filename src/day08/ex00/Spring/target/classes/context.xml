<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xmlns:context="http://www.springframework.org/schema/context"
  xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
    http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context.xsd">

  <bean id="preProcessorUp" class="src.main.java.school21.spring.preprocessors.PreProcessorToUpperImpl"/>
  <bean id="preProcessorLow" class="src.main.java.school21.spring.preprocessors.PreProcessorToLowerImpl"/>

  <bean id="renderErrUp" class="src.main.java.school21.spring.renders.RenderErrImpl">
    <constructor-arg ref="preProcessorUp"/>
  </bean>
  <bean id="renderErrLow" class="src.main.java.school21.spring.renders.RenderErrImpl">
    <constructor-arg ref="preProcessorLow"/>
  </bean>
  <bean id="renderStdUp" class="src.main.java.school21.spring.renders.RenderStandardImpl">
    <constructor-arg ref="preProcessorUp"/>
  </bean>
  <bean id="renderStdLow" class="src.main.java.school21.spring.renders.RenderStandardImpl">
    <constructor-arg ref="preProcessorLow"/>
  </bean>

  <bean id="printerWithDateErrLow" class="src.main.java.school21.spring.printers.PrinterWithDateTimeImpl">
    <constructor-arg ref="renderErrLow"/>
  </bean>

  <bean id="printerWithDateStdUp" class="src.main.java.school21.spring.printers.PrinterWithDateTimeImpl">
    <constructor-arg ref="renderStdUp"/>
  </bean>

  <bean id="printerWithPrefixStdUp" class="src.main.java.school21.spring.printers.PrinterWithPrefixImpl">
    <constructor-arg ref="renderStdUp"/>
    <property name="prefix" value="Prefix"/>
  </bean>

  <bean id="printerWithPrefixErrUp" class="src.main.java.school21.spring.printers.PrinterWithPrefixImpl">
    <constructor-arg ref="renderErrUp"/>
    <property name="prefix" value="Prefix"/>
  </bean>

</beans>
